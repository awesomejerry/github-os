{
  "name": "github-os",
  "version": "2.2.4",
  "description": "A web-based terminal interface that mimics a VM file system, browsing GitHub repositories as directories and files.",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awesomejerry/github-os.git"
  },
  "keywords": [
    "github",
    "terminal",
    "filesystem",
    "cli"
  ],
  "author": "awesomejerry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/awesomejerry/github-os/issues"
  },
  "homepage": "https://github.com/awesomejerry/github-os#readme",
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@vitest/coverage-v8": "^4.0.18",
    "jsdom": "^28.1.0",
    "playwright": "^1.58.2",
    "vitest": "^4.0.18"
  }
}
